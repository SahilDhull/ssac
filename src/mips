.data
		str1: .asciiz "----\n" 
.text
.globl main
main:
	addi $fp, $sp, 0
	addi $sp, $sp, -16
	lw $5, -4($fp)
	li $5, 2
	lw $6, -12($fp)
	li $6, 0
	lw $7, -8($fp)
	sub $7, $7, $6
	lw $8, -16($fp)
	move $8, $7
	li $2, 1
	move $4, $8
	syscall
	sw $5, -4($fp)
	sw $6, -12($fp)
	sw $7, -8($fp)
	sw $8, -16($fp)
	addi $sp, $sp, 16
	jr $ra
